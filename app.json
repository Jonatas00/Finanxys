{
  "expo": {
    "name": "Finanxys",
    "slug": "Finanxys",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "finanxys",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "package": "com.jonatas00.Finanxys",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "native",
          "backgroundColor": "#262626"
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cb1819e2-021f-4b5a-9152-0c12e6e60e71"
      }
    }
  }
}
